<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item 
		android:left="4dp" 
	    android:right="0dp" 
	 	android:top="6dp" 
	    android:bottom="0dp">
	    <shape 
	    	android:shape="rectangle" >
		    <corners 
		    	android:radius="10dip" />
		    <gradient
			    android:angle="0"
			    android:startColor="#50000000"
			    android:endColor="#4B000000"
			    android:type="linear" />
		</shape>
	</item>
	<item 
	    android:top="0dp" 
	    android:bottom="6dp"
	    android:right="4dp" 
	    android:left="0dp" >
	    <shape 
	    	android:shape="rectangle" >
		    <solid 
		    	android:color="#FFFFFFFF" />
		    <stroke 
		    	android:width="1dip" 
		    	android:color="#FF000000"/>
		    <corners 
		    	android:radius="10dip" />
		    <padding 
		    	android:left="7dip" 
		        android:right="7dip" 
		    	android:top="7dip"
		        android:bottom="7dip" />
		</shape>
	</item>
</layer-list>